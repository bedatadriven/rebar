          <form class="{b.navbarForm} {b.navbar}" role="form">
            <div class="{b.formGroup}">
              <input type="text" placeholder="Email" class="{b.formControl}"/>
            </div>
            <div class="{b.formGroup}">
              <input type="password" placeholder="Password" class="{b.formControl}"/>
            </div>
            <button type="submit" class="{b.btn} {b.btnSuccess}">Sign in</button>
          </form>
        </div><!--/.navbar-collapse -->
      </div>
    </div>

