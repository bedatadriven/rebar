    <div class="{b.navbar} {b.navbarInverse} {b.navbarFixedTop}" role="navigation">
      <div class="{b.container}">
        <div class="{b.navbarHeader}">
          <button type="button" class="{b.navbarToggle}"
            data-toggle="collapse" data-target=".navbar-collapse">
